<%= form_for(@restaurant) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :address %>
<%= f.text_field :address %>

<%= f.label :phonenumber %>
<%= f.text_field :phonenumber %>

<%= f.select(:category, options_for_select([['French', 'French'], ['Italian', "Italian"], ['Chinese', "Chinese"],['Japanese', "Japanese"], ['Belgian', "Belgian"]])) %>
<%= f.submit %>
<% end %>
